<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="food">
	<insert id="insertOfFood" parameterType="fdto">
		insert into food (photoname, su, dan) values (#{photoname}, #{su}, #{dan})
	</insert>
	
	<select id="AllOfFood" resultType="fdto">
		select * from food
	</select>
</mapper>